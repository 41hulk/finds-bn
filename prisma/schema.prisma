generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String   @id @default(cuid())
  email       String   @unique
  password    String
  username    String   @unique
  profileUrl  String?
  nationality String
  instagram   String?
  twitter     String?
  linkedin    String?
  created_at  DateTime @default(now())
}
